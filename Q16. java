public class MIT {
    public static void main(String[] args) {
        student i = new student();

        i.set_details("Ande",120154055,"Electrical & Electronics");
        i.print_details();
    }


}
class student{
    private String name;
    private int regNo;
    private String enggBranch;
    public void print_details(){
        System.out.println(name);
        System.out.println(regNo);
        System.out.println(enggBranch);
    }
    public void set_details(String a, int b, String c){
        name      = a ;
        regNo     = b;
        enggBranch=c;
    }
}
