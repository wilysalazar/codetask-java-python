import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner Value = new Scanner(System.in);
        System.out.println("Enter your desired string, integer, char and double");

        String myString = Value.nextLine();
        int integer = Value.nextInt();
        char character = Value.next().charAt(0);
        double ddouble = Value.nextDouble();

        System.out.println("String: " + myString);
        System.out.println("Integer: " + integer);
        System.out.println("Character: " + character);
        System.out.println("Double: " + ddouble);
    }
}
